<!DOCTYPE html>
<html>
  <head>
    <title>CJ Fashion House</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar navbar-toggleable-md navbar-inverse bg-inverse">
      <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo02" aria-controls="navbarTogglerDemo02" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <%= link_to 'CJ Fashion House', root_path, class: 'navbar-brand' %>
      <div class="container">
       <b>Administrator</b>
      <br />
      <%= link_to 'Elegant Styles', new_page_style_path %>
      </div>
</div>  

    </nav>
       <% if notice.present? %>
          <p class="alert alert-info"><%= notice %></p>
        <% end %>
        <% if alert.present? %>
          <p class="alert alert-danger"><%= alert %></p>
        <% end %>
       <%= yield %>   

    <div id="footer">
      <div class="container">
      <div class="row">
      <div class="col-12 col-sm-3">
       
      </div>
      <div class="col-12 col-sm-3">
        <%= link_to 'About', '#' %>
        <br />
        <%= link_to 'Privacy Policy', '#' %>
      </div>
      <div class="col-12 col-sm-3">
        <b>Contact</b>
        <br />
        CJfashionhouse@gmail.com
        <br />
        204 Anderson Square
        <br />
        Austin, TX 78753
      </div>
      <div class="col-12 col-sm-3">
        <b>Social</b>
         <br />
          <%= link_to 'https://github.com/bigvee-design' do %>
            <span class="fa fa-github-square fa-2x social-button"></span>
          <% end %>
          <%= link_to 'https://www.facebook.com/vmjob?ref=bookmarks' do %>
            <span class="fa fa-facebook-square fa-2x social-button"></span>
          <% end %>
          <%= link_to 'https://twitter.com/Vincent_Job' do %>
            <span class="fa fa-twitter-square fa-2x social-button"></span>
         <% end %>
      </div>
      </div>
     </div>
    </div>
  </body>
</html>
