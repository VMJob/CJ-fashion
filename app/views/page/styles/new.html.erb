<div class="booyah-box col-10 offset-1">
  <h1>Add a new style</h1>
  <%= simple_form_for @style, url: page_styles_path do |f| %>
    <%= f.input :selection %>
    <%= f.input :measurement %>
    <br />
    <%= f.submit "Add Your Style", class: 'btn btn-primary' %>
    <br />
  <% end %>
  <br />
</div>
 