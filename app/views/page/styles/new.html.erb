<br />
<div class="booyah-box col-10 offset-1">
  <h1>Add a new style</h1>
  <%= simple_form_for @style, url: new_style_path do |f| %>
    <%= f.input :style %>
    <%= f.input :measurement %>
    <%= f.input :cost %>
    <br />
    <%= f.submit "Create", class: 'btn btn-primary' %>
    <br />
  <% end %>
  <br />
</div>
 
</div>